OPENAI_API_KEY=$(grep "^OPENAI_API_KEY=" .env | cut -d'=' -f2- | tr -d '"')
OPENAI_BASE_URL=$(grep "^OPENAI_BASE_URL=" .env | cut -d'=' -f2- | tr -d '"')
OPENAI_MODEL=$(grep "^OPENAI_MODEL=" .env | cut -d'=' -f2- | tr -d '"')

AUTH_USERNAME=$(grep "^AUTH_USERNAME=" .env | cut -d'=' -f2- | tr -d '"')
AUTH_PASSWORD=$(grep "^AUTH_PASSWORD=" .env | cut -d'=' -f2- | tr -d '"')

